      },
      {
        "viewport": "stripe.dashboard.payment.detail",
        "component": "PaymentDetailView"
      },
      {
        "viewport": "stripe.dashboard.customer.list",
        "component": "CustomerListView"
      },
      {
        "viewport": "stripe.dashboard.customer.detail",
        "component": "CustomerDetailView"
      },
      {
        "viewport": "stripe.dashboard.invoice.list",
        "component": "InvoiceListView"
      },
      {
        "viewport": "stripe.dashboard.invoice.detail",
        "component": "InvoiceDetailView"
      },
      {
        "viewport": "stripe.dashboard.product.list",
        "component": "ProductListView"
      },
      {
        "viewport": "stripe.dashboard.product.detail",
        "component": "ProductDetailView"
      },
      {
        "viewport": "stripe.dashboard.subscription.list",
        "component": "SubscriptionListView"
      },
      {
        "viewport": "stripe.dashboard.subscription.detail",
        "component": "SubscriptionDetailView"
