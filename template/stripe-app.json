    {
      "permission": "customer_read",
      "purpose": "list customer detail"
    },
    {
      "permission": "payment_intent_read",
      "purpose": "read PI"
    }
  ],
  "app_backend": {
    "webhooks": null
  },
  "ui_extension": {
    "views": [
      {
        "viewport": "stripe.dashboard.payment.list",
