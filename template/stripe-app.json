      },
      {
        "viewport": "stripe.dashboard.report.overview",
        "component": "ReportOverviewView"
      },
      {
        "viewport": "stripe.dashboard.revenue-recognition.overview",
        "component": "RevenueRecognitionOverviewView"
      },
      {
        "viewport": "stripe.dashboard.tax-report.overview",
        "component": "TaxReportOverviewView"
